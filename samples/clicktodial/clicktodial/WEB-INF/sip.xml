<?xml version="1.0" encoding="UTF-8"?>
  <sip-app  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://www.jcp.org/xml/ns/sipservlet sip-app_1_1.xsd" >
    <app-name>click-to-dial</app-name>
    <display-name>click-to-dial</display-name>
   
    <description>Sample Click-to-Dial Application</description>
    <!-- Management link:clicktodial/WebAction -->

  <servlet>
    <servlet-name>SipAction</servlet-name>
    <servlet-class>com.micromethod.sipmethod.sample.clicktodial.SipAction</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-selection>
    <servlet-mapping>
      <servlet-name>SipAction</servlet-name>
      <pattern>
          <and>
            <equal ignore-case="false">
              <var>request.method</var>
              <value>REGISTER</value>
            </equal>
            <equal ignore-case="false">
              <var>request.from.uri.host</var>
              <value>clicktodial.sample.prism.voxeo.com</value>
            </equal>
          </and>
      </pattern>
    </servlet-mapping>
  </servlet-selection>
</sip-app>
