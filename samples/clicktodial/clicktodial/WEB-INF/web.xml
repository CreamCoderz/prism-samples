<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <display-name>click-to-dial</display-name>
    <description>Sample Click-to-Dial Application</description>

    <servlet>
        <servlet-name>WebAction</servlet-name>
        <servlet-class>com.micromethod.sipmethod.sample.clicktodial.WebAction</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>WebAction</servlet-name>
        <url-pattern>/WebAction</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>WebServiceServlet</servlet-name>
        <url-pattern>/makecall</url-pattern>
    </servlet-mapping>
</web-app>

